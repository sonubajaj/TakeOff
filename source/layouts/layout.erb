<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <meta name="viewport" content="width=device-width, user-scalable=no">
    <meta name="description" content="TakeOff | We ideate. We design. We build. We grow.">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= "TakeOff - Aim your venture to success" %></title>

    <%= stylesheet_link_tag "all" %>
    <script src="//ajax.googleapis.com/ajax/libs/webfont/1.5.2/webfont.js"></script>
    <script>
      WebFont.load({
        google: {
          families: ['Dosis', 'Sorts Mill Goudy']
        }
      });
    </script>
    <% if build? %>
      <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-46160613-2', 'takeoff.net.in');
        ga('send', 'pageview');
      </script>
    <% end %>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.11.5/TweenMax.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.11.5/plugins/ScrollToPlugin.min.js"></script>
    <%= javascript_include_tag  "all" %>
  </body>
</html>
